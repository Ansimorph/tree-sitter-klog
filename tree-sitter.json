{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "klog",
      "camelcase": "Klog",
      "title": "Klog",
      "scope": "source.klog",
      "file-types": [
        "klg"
      ],
      "injection-regex": "^klog$",
      "class-name": "TreeSitterKlog"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "tree-sitter parser for the klog time-tracking language",
    "authors": [
      {
        "name": "Bj√∂rn Ganslandt",
        "email": "hi@ganslandt.xyz",
        "url": "https://ganslandt.xyz"
      }
    ],
    "links": {
      "repository": "https://github.com/ansimorph/tree-sitter-klog",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}