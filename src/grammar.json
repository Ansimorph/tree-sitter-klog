{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "klog",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "record"
      }
    },
    "_summary_text": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_word"
          },
          {
            "type": "SYMBOL",
            "name": "tag"
          }
        ]
      }
    },
    "_word": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^#\\s]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^#\\s]+"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "SYMBOL",
          "name": "tag_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "IMMEDIATE_TOKEN",
                  "content": {
                    "type": "STRING",
                    "value": "="
                  }
                },
                {
                  "type": "SYMBOL",
                  "name": "tag_value"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "tag_name": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[\\p{L}\\d_-]+",
        "flags": "u"
      }
    },
    "tag_value": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "((\"[^\"]*\")|('[^']*')|([\\p{L}\\d_-]*))",
        "flags": "u"
      }
    },
    "time": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "time_association"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_12hour_time"
            },
            {
              "type": "SYMBOL",
              "name": "_24hour_time"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "time_association"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_12hour_time": {
      "type": "PATTERN",
      "value": "(0?[1-9]|1[0-2]):[0-5][0-9](am|pm)"
    },
    "_24hour_time": {
      "type": "PATTERN",
      "value": "(24:00|([01]?[0-9]|2[0-3]):[0-5][0-9])"
    },
    "time_association": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "STRING",
          "value": ">"
        }
      ]
    },
    "open_range": {
      "type": "PATTERN",
      "value": "\\?+"
    },
    "range": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "time"
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "time"
            },
            {
              "type": "SYMBOL",
              "name": "open_range"
            }
          ]
        }
      ]
    },
    "duration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "positive_duration"
        },
        {
          "type": "SYMBOL",
          "name": "negative_duration"
        }
      ]
    },
    "_duration_string": {
      "type": "PATTERN",
      "value": "\\d+h\\d+m|\\d+h|\\d+m"
    },
    "positive_duration": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_duration_string"
        }
      ]
    },
    "negative_duration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "SYMBOL",
          "name": "_duration_string"
        }
      ]
    },
    "date": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_slash_date"
        },
        {
          "type": "SYMBOL",
          "name": "_dash_date"
        }
      ]
    },
    "_slash_date": {
      "type": "PATTERN",
      "value": "\\d{4}\\/\\d{2}\\/\\d{2}"
    },
    "_dash_date": {
      "type": "PATTERN",
      "value": "\\d{4}-\\d{2}-\\d{2}"
    },
    "record": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_record_header"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "record_summary"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          },
          "named": true,
          "value": "summary"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_indent"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SYMBOL",
                    "name": "entry"
                  }
                },
                {
                  "type": "SYMBOL",
                  "name": "_dedent"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_record_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "date"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": " +"
                },
                {
                  "type": "SYMBOL",
                  "name": "should_total"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "should_total": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "duration"
        },
        {
          "type": "STRING",
          "value": "!)"
        }
      ]
    },
    "record_summary": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_summary_text"
          },
          {
            "type": "SYMBOL",
            "name": "_newline"
          }
        ]
      }
    },
    "entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "duration"
            },
            {
              "type": "SYMBOL",
              "name": "range"
            }
          ]
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "entry_summary"
          },
          "named": true,
          "value": "summary"
        }
      ]
    },
    "entry_summary": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_newline"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": " "
                },
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "_entry_summary_inline"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_entry_summary_indented"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_entry_summary_inline": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_summary_text"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_entry_summary_indented": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_summary_text"
              },
              {
                "type": "SYMBOL",
                "name": "_newline"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "_dedent"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_newline"
    },
    {
      "type": "SYMBOL",
      "name": "_indent"
    },
    {
      "type": "SYMBOL",
      "name": "_dedent"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}